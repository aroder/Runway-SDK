      com.runwaysdk.web.MojaxFilter
    </filter-class>
  </filter>

  <filter-mapping>
    <filter-name>
      MojaxFilter
    </filter-name>
    <url-pattern>
      *.mojax
    </url-pattern>
  </filter-mapping>

  <filter>
    <filter-name>
      MofoFilter
    </filter-name>
    <filter-class>
      com.runwaysdk.web.MofoFilter
    </filter-class>
  </filter>

  <filter-mapping>
    <filter-name>
      MofoFilter
    </filter-name>
    <url-pattern>
      *.mofo
    </url-pattern>
  </filter-mapping>


</web-app>
